func main()
  var a,b,c: int
  var q : bool
  var A: array[10] of int
  var B: array[10] of int
  var C: array[10] of bool

  A[c] = B[q] - 1;
  A = [ true ? b+1 : 0 for b in B ];
  if B[q] == C[b*a] then
     B = [ c>=0 ? a*c : b*(-c+1) for c in B ];
  endif
  C[3] = q or A[3.2+b]==0;
  C = [ a>c ? a%2==0 : a*b>5 for a in A ];
  B = [ q or b!=3 ? b*2 : b/5 for q in C];
  b[5] = A[B[2]] + B[A];
endfunc
